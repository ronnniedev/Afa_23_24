package ficheros;

import java.util.Scanner;

public class Ejercicio3 {
	static Scanner keyboard = new Scanner(System.in);
	public static void main(String[] args) {
		
		System.out.println("Cuantas filas tiene la matriz: ");
		int filas = keyboard.nextInt();
		
		System.out.println("Cuantas columnas tiene la matriz: ");
		int columnas = keyboard.nextInt();
		
		int m [][] = Libreria.generarMatrizInt(filas, columnas, 1, 9);
		
		Libreria.mostrarMatriz(m);
		System.out.println("----------->Reloj<------------");
		Libreria.reloj(m);

	}

}
