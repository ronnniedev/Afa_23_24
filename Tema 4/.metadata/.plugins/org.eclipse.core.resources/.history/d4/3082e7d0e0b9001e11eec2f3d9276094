package ejercicio2;

public class Libreria {
	
	/** genera un numero entre el max y min especificado
	 * @param min
	 * @param max
	 * @return entero
	 */
	public static int generaNumeros (int min,int max) {
		int num = (int) (Math.random()*max)+min;
		System.out.println(num);
		return (int) Math.random()*max+min;
		
	}

}
