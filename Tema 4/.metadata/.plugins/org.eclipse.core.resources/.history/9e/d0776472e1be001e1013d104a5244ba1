package ejercicio4;

public class Computadoras {

	private String marca;
	private int memoria;
	private String iso;
	private int precio;
	
	
	public Computadoras(String marca, int memoria, String iso, int precio) {
		this.marca = marca;
		this.memoria = memoria;
		this.iso = iso;
		this.precio = precio;
	}


	public String toString() {
		return "Computadoras [marca=" + marca + ", memoria=" + memoria + ", iso=" + iso + ", precio=" + precio + "]";
	}


	public Computadoras() {
		
	}
	
	
	
	
	
	
	
}
